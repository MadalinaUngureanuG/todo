{
  "extends": "next/core-web-vitals",
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "arrow-parens":  [true, "ban-single-arg-parens", "error", "as-needed"], 
     "react/no-unescaped-entities": "off",
     "@next/next/no-page-custom-font": "off"
  }
}
